<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="esprit.tn.controllers.CodeQR"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="QRCodebtn" layoutX="14.0" layoutY="74.0" mnemonicParsing="false" onAction="#generateQrCode" prefHeight="26.0" prefWidth="164.0" text="Code Qr" />
        <ImageView fx:id="QRCode" fitHeight="150.0" fitWidth="200.0" layoutX="232.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" />
        </children>

</AnchorPane>
